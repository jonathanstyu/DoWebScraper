{
  "manifest_version": 2,

  "name": "Texter",
  "description": "Helps me set up stuff",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Stuff"
  },

  "background": {
    "scripts":["mybackground.js"]
  },

  "web_accessible_resources": ["page.html"],

  "permissions": [
    "tabs",
    "activeTab",
    "pageCapture"
  ]
}
