{
  "manifest_version": 3,

  "name": "DoWebScraper",
  "description": "Turn structured HTML into a CSV or a TXT file.",
  "version": "2.0.0",

  "action": {
    "default_icon": "ScraperLogo.png",
    "default_title": "DoWebScraper"
  },

  "background": {
    "service_worker": "mybackground.js"
  },

  "web_accessible_resources": [{
    "resources": ["page.html"],
    "matches": ["<all_urls>"]
  }],

  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],

  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
