{
  "manifest_version": 2,

  "name": "PageScraper",
  "description": "Turn structured HTML into a CSV or a TXT file.",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Stuff"
  },

  "background": {
    "scripts":["mybackground.js"]
  },

  "web_accessible_resources": ["page.html"],

  "permissions": [
    "tabs",
    "activeTab",
    "pageCapture"
  ]
}
